<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">mundipagg_mundipagg_subscriptions_listing.mundipagg_mundipagg_subscriptions_listing_data_source</item>
			<item name="deps" xsi:type="string">mundipagg_mundipagg_subscriptions_listing.mundipagg_mundipagg_subscriptions_listing_data_source</item>
		</item>
		<item name="spinner" xsi:type="string">spinner_columns</item>
	</argument>

	<listingToolbar name="listing_top">
		<argument name="data" xsi:type="array">
			<item name="config" xsi:type="array">
				<item name="sticky" xsi:type="boolean">true</item>
			</item>
		</argument>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters" />
		<paging name="listing_paging"/>
	</listingToolbar>

	<dataSource name="nameOfDataSource">
		<argument name="dataProvider" xsi:type="configurableObject">
			<argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider</argument>
			<argument name="name" xsi:type="string">mundipagg_mundipagg_subscriptions_listing_data_source</argument>
			<argument name="primaryFieldName" xsi:type="string">id</argument>
			<argument name="requestFieldName" xsi:type="string">id</argument>
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
					<item name="update_url" xsi:type="url" path="mui/index/render"/>
					<item name="storageConfig" xsi:type="array">
						<item name="indexField" xsi:type="string">id</item>
					</item>
				</item>
			</argument>
		</argument>
	</dataSource>
	<columns name="spinner_columns">
		<column name="id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">textRange</item>
					<item name="sorting" xsi:type="string">asc</item>
					<item name="label" xsi:type="string" translate="true">ID</item>
				</item>
			</argument>
		</column>
		<column name="mundipagg_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Mundipagg id</item>
				</item>
			</argument>
		</column>
		<column name="code">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Code</item>
				</item>
			</argument>
		</column>
		<column name="status" class="MundiPagg\MundiPagg\Ui\Component\Listing\Column\Translate">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Status</item>
				</item>
			</argument>
		</column>
		<column name="installments" class="MundiPagg\MundiPagg\Ui\Component\Listing\Column\Translate">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Allow Installments</item>
				</item>
			</argument>
		</column>
		<column name="payment_method" class="MundiPagg\MundiPagg\Ui\Component\Listing\Column\Translate">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Payment method</item>
				</item>
			</argument>
		</column>
		<column name="recurrence_type">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Recurrence type</item>
				</item>
			</argument>
		</column>
		<column name="interval_type">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Interval type</item>
				</item>
			</argument>
		</column>
		<column name="interval_count">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">interval Count</item>
				</item>
			</argument>
		</column>
		<actionsColumn name="actions" class="MundiPagg\MundiPagg\Ui\Component\Column\Subscriptions\Actions">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="indexField" xsi:type="string">id</item>
				</item>
			</argument>
		</actionsColumn>
	</columns>
</listing>