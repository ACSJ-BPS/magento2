{
    "name": "pagarme/magento2-module",
    "license": "MIT",
    "version": "1.0.0",
    "type": "magento2-module",
    "description": "Magento 2 Module Pagar.me",
    "require": {
        "php": ">=7.1.0",
        "mundipagg/mundiapi": "^3.0",
        "pagarme/ecommerce-module-core": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2.4",
        "squizlabs/php_codesniffer": "3.0.1",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.5.0",
        "sjparkinson/static-review": "~4.1",
        "fabpot/php-cs-fixer": "~1.2",
        "lusitanian/oauth": "~0.3 <=0.8.10",
        "phpunit/phpunit-selenium": ">=1.2",
        "sebastian/phpcpd": "^2.0",
        "satooshi/php-coveralls": "^1.0",
        "magento-ecg/coding-standard":"dev-master",
        "rregeer/phpunit-coverage-check": "^0.1.3",
        "mundipagg/mundiapi": "^3.0"
    },
    "authors":[
        {
            "name":"Open Source Team"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "files": [
            "app/code/Pagarme/Pagarme/registration.php"
        ],
        "psr-4": {
            "Pagarme\\Pagarme\\": ""
        }
    },
    "config":{
        "secure-http":false
    }
}
